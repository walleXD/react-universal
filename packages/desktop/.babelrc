{
  "presets": [
    "stage-0",
    "react-native"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      {
        "extensions": [
          ".web.js",
          ".js",
          ".jsx",
          ".mjs"
        ],
        "alias": {
          "react-native": "react-native-web",
          "react/lib/ReactNativePropRegistry": "react-native-web/dist/modules/ReactNativePropRegistry",
          "@universal-react/components": "@universal-react/components/web"
        }
      }
    ]
  ]
}
