{
  "presets": [
    "es2017",
    "stage-0",
    "react-native"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [
          ".web.js",
          ".js",
          ".jsx",
          ".mjs"
        ],
        "root": [
          "./src"
        ]
      }
    ],
    "react-native-web/babel"
  ]
}
